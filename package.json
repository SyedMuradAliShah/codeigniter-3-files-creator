{
    "name": "codeigniter-3-files-creator",
    "displayName": "Codeigniter 3 Files Creator",
    "description": "Creating files never been so easy, with the help of CI3 Files Creator, you can create auth, crud, model, controller, migration etc with a few clicks.",
    "icon": "assets/icon.png",
    "version": "0.2.7",
    "publisher": "SyedMuradAliShah",
    "repository": "https://github.com/SyedMuradAliShah/codeigniter-3-files-creator",
    "author": "Syed Murad Ali Shah <murad@receivemail.org> (http://focdownloads.blogspot.com/)",
    "license": "(ISC OR GPL-3.0)",
    "funding": {
        "type": "patreon",
        "url": "https://www.patreon.com/FocDownloads"
    },
    "engines": {
        "vscode": "^1.53.2"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:make.ci3_view",
        "onCommand:make.ci3_library_google_authenticator",
        "onCommand:make.ci3_hook",
        "onCommand:make.ci3_template",
        "onCommand:make.ci3_migration_add_table",
        "onCommand:make.ci3_migration_rename_table",
        "onCommand:make.ci3_migration_add_column",
        "onCommand:make.ci3_migration_rename_column",
        "onCommand:make.ci3_simple_auth",
        "onCommand:make.ci3_simple_auth_api",
        "onCommand:make.ci3_crud",
        "onCommand:make.ci3_controller",
        "onCommand:make.ci3_model",
        "onCommand:make.ci3_helper",
        "onCommand:make.ci3_library",
        "onCommand:make.ci3_language",
        "onCommand:make.ci3_htaccess"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "make.ci3_view",
                "title": " CI3 make:view"
            },
            {
                "command": "make.ci3_library_google_authenticator",
                "title": " CI3 make:library_google_authenticator"
            },
            {
                "command": "make.ci3_hook",
                "title": " CI3 make:hook"
            },
            {
                "command": "make.ci3_template",
                "title": " CI3 make:template"
            },
            {
                "command": "make.ci3_migration_add_table",
                "title": " CI3 make:migration:add_table"
            },
            {
                "command": "make.ci3_migration_rename_table",
                "title": " CI3 make:migration:rename_table"
            },
            {
                "command": "make.ci3_migration_add_column",
                "title": " CI3 make:migration:add_column"
            },
            {
                "command": "make.ci3_migration_rename_column",
                "title": " CI3 make:migration:rename_column"
            },
            {
                "command": "make.ci3_simple_auth",
                "title": " CI3 make:simple_auth"
            },
            {
                "command": "make.ci3_simple_auth_api",
                "title": " CI3 make:simple_auth_api"
            },
            {
                "command": "make.ci3_crud",
                "title": " CI3 make:crud"
            },
            {
                "command": "make.ci3_model",
                "title": " CI3 make:model"
            },
            {
                "command": "make.ci3_controller",
                "title": " CI3 make:controller"
            },
            {
                "command": "make.ci3_helper",
                "title": " CI3 make:helper"
            },
            {
                "command": "make.ci3_library",
                "title": " CI3 make:library"
            },
            {
                "command": "make.ci3_language",
                "title": " CI3 make:language"
            },
            {
                "command": "make.ci3_htaccess",
                "title": " CI3  make:htaccess"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "eslint": "^8.17.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "vsce": "^2.9.1"
    }
}
